# PHPeek PM Example Environment File
# Copy to .env and customize for your setup

###########################################
# Application
###########################################
APP_KEY=base64:your-32-character-key-here
APP_URL=http://localhost

###########################################
# Database
###########################################
MYSQL_ROOT_PASSWORD=root-password-change-me
DB_PASSWORD=laravel-password-change-me

###########################################
# Redis
###########################################
REDIS_PASSWORD=redis-password-change-me

###########################################
# PHPeek PM API
###########################################
PHPEEK_API_TOKEN=your-secure-token-change-me

###########################################
# Grafana
###########################################
GRAFANA_PASSWORD=admin-change-me

###########################################
# Optional: Override Process Settings
###########################################
# PHPEEK_PM_PROCESS_QUEUE_DEFAULT_SCALE=5
# PHPEEK_PM_PROCESS_QUEUE_HIGH_SCALE=3
# PHPEEK_PM_LOG_LEVEL=debug
